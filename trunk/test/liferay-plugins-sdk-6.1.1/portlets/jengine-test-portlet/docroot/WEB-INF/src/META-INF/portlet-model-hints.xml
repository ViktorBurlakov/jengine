<model-hints>
	<model name="com.jenginetest.builder.model.SAuthor">
		<field name="authorId" type="long" />
		<field name="firstName" type="String" />
		<field name="lastName" type="String" />
	</model>
	<model name="com.jenginetest.builder.model.SBook">
		<field name="bookId" type="long" />
		<field name="libraryId" type="long" />
		<field name="title" type="String" />
	</model>
	<model name="com.jenginetest.builder.model.SLibrary">
		<field name="libraryId" type="long" />
		<field name="name" type="String" />
		<field name="address" type="String" />
	</model>
	<model name="com.jenginetest.builder.model.SMember">
		<field name="memberId" type="long" />
		<field name="libraryId" type="long" />
		<field name="firstName" type="String" />
		<field name="lastName" type="String" />
	</model>
	<model name="com.jenginetest.builder.model.STransaction">
		<field name="transactionId" type="long" />
		<field name="bookId" type="long" />
		<field name="memberId" type="long" />
		<field name="tdate" type="Date" />
	</model>
</model-hints>